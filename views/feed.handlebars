<div class="feed">
     {{#if loggedIn}}
         <!-- Post #1 -->
    <div class="tile is-4 is-vertical is-parent">
        <div class="tile is-child box">
            <p> {{post1.title}}</p>
            <img src="{{post1.img_url}}" alt="{{post1.title}}" />
            <p>created by
                {{post1.user.username}}
                on
                {{format_date post1.created_at}}</p>
