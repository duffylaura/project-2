{{#if loggedIn}}
    {{#each post}}  <!-- Fix this styling after get static homepage --> 
    <div class="tile is-4 is-parent">
        <div class="tile is-child box">
            <p> {{post.title}}</p>
            <img src="{{post.img_url}}" alt="{{post.title}}" />
            <p>created by
                {{post.user.username}}
                on
                {{format_date post.created_at}}</p>
        </div>
    </div>
    {{/each}}

    {{else}}
    <br> <br>

    <div class = "tile">
